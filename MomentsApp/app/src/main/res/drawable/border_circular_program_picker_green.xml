<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="5dp"/>
            <solid android:color="@color/program_color_green" />
            <stroke android:width="1dp" android:color="@color/program_color_green" />
            <corners android:radius="5dp" /></shape>
    </item>
    <item android:left="6dp">
        <shape android:shape="rectangle">
            <size android:width="5dp"/>
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="@color/alice_blue" />
            <corners android:radius="1dp" />
        </shape>
    </item>
</layer-list>